<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 630" width="1200" height="630">
  <!-- Background -->
  <rect width="1200" height="630" fill="#f9fafb"/>
  
  <!-- Gradient Overlay -->
  <defs>
    <linearGradient id="grad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2563eb;stop-opacity:0.1" />
      <stop offset="100%" style="stop-color:#1e40af;stop-opacity:0.1" />
    </linearGradient>
  </defs>
  <rect width="1200" height="630" fill="url(#grad)"/>
  
  <!-- Large Tool Icon -->
  <g transform="translate(200, 150)">
    <!-- Hammer -->
    <rect x="100" y="80" width="120" height="50" fill="#2563eb" opacity="0.3" rx="5"/>
    <rect x="140" y="130" width="15" height="200" fill="#6b7280" opacity="0.3" rx="5"/>
  </g>
  
  <g transform="translate(350, 200)">
    <!-- Wrench -->
    <rect x="0" y="0" width="60" height="30" fill="#1e40af" opacity="0.3" rx="3"/>
    <rect x="48" y="8" width="24" height="14" fill="#2563eb" opacity="0.3"/>
    <rect x="0" y="11" width="160" height="8" fill="#1e40af" opacity="0.3"/>
  </g>
  
  <!-- Main Text -->
  <text x="600" y="280" font-family="Arial, sans-serif" font-size="80" font-weight="bold" fill="#1e40af" text-anchor="middle">
    PUNCH LIST
  </text>
  <text x="600" y="360" font-family="Arial, sans-serif" font-size="60" fill="#6b7280" text-anchor="middle">
    HANDYMAN
  </text>
  
  <!-- Tagline -->
  <text x="600" y="440" font-family="Arial, sans-serif" font-size="32" fill="#6b7280" text-anchor="middle">
    Professional Home Repair Services
  </text>
  
  <!-- Website -->
  <text x="600" y="520" font-family="Arial, sans-serif" font-size="28" fill="#2563eb" text-anchor="middle">
    www.punch-list-handyman.com
  </text>
</svg>
